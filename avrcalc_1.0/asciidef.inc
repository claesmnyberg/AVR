;* Control characters
.EQU NUL   =   0        ; NULL, used for fillout
.EQU SOH   =   1        ; Start Of Header (^A)
.EQU STX   =   2        ; Start Of Text (^B)
.EQU ETX   =   3        ; End Of Text (^C)
.EQU EOT   =   4        ; End Of Transmission (^D)
.EQU ENQ   =   5        ; Enquiry (^E)
.EQU ACK   =   6        ; Positive Acknowledgement (^F)
.EQU BEL   =   7        ; Bell / Alarm (^G)
.EQU BS    =   8        ; Backspace (^H)
.EQU HT    =   9        ; Horizontal Tab (^I)
.EQU LF    =  10        ; Line Feed (^J)
.EQU VT    =  11        ; Vertical Tab (^K)
.EQU FF    =  12        ; Form Feed (^L)
.EQU CR    =  13        ; Carriage Return (^M)
.EQU SO    =  14        ; Shift Out (^N)
.EQU SI    =  15        ; Shift In (^O)
.EQU DLE   =  16        ; Data Link Escape (^P)
.EQU DCI   =  17        ; Device Control 1 (^Q) (Same as XON below)
.EQU XON   =  17        ; Transmit on (^Q), Start special function
.EQU DC2   =  18        ; Device Control 2 (^R)
.EQU DC3   =  19        ; Device Control 3 (^S) (Same as XOFF below)
.EQU XOFF  =  19        ; Transmit Off (^S), Makes a terminal stop transmit
.EQU DC4   =  20        ; Device Control 4 (^T)
.EQU NAK   =  21        ; Negative Acknowledgement (^U)
.EQU SYN   =  22        ; Synchronous idle (^V)
.EQU ETB   =  23        ; End Of Transmission Block (^W)
.EQU CAN   =  24        ; Cancel (^X)
.EQU EM    =  25        ; End Of Medium (^Y), Fysical end. i.e. end of tape
.EQU SUB   =  26        ; Substitute (^Z)
.EQU ESC   =  27        ; Escape (^])
.EQU FS    =  28        ; File Separator (^\)
.EQU GS    =  29        ; Group Separator (^])
.EQU RS    =  30        ; Record Separator
.EQU US    =  31        ; Unit Separator
.EQU SPACE =  32        ; Space 

